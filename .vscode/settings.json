{
    "rest-client.environmentVariables": {
        "$shared": {
            "prefix": "api",
            "v1": "v/1",
            "v2": "v/2"
        },
        "localhost": {
            "http-host": "http://localhost:5000",
            "https-host": "https://localhost:5000",
            "token": "Bearer ",
        },
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "[typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
}